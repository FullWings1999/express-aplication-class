{{#each articales}}
    {{#if this.publish}}
        <section class="articale">
            <h3><a href="{{../backUrl}}">Go back</a> </h3>
            <br / >
            {{#with this}}
                <h2 class="articale-title">
                    {{key}}{{title}}
                </h2>
                <h3 class="articale-subTitle">
                    {{subTitle}}
                </h3>
                <h4 class="articale-date">
                    {{creatDate}}
                </h4>
                <p class="articale-content">
                    {{content}}
                </p>
            {{/with}}
        </section>
    {{/if}}


{{/each}}